    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "tslib": "^2.4.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  },
  "type": "module",
  "dependencies": {
    "@svelteuidev/composables": "^0.15.6",
